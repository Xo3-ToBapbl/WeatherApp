{
  "name": "weather-app",
  "description": "Simple weather web application",
  "version": "0.0.1",
  "author": "Ivan Stupak",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --config build/webpack.config.js",
    "watch": "webpack --watch --config build/webpack.config.js --env dev",
    "start_server": "webpack serve --config build/webpack.config.js --env dev",
    "clean": "rm -r ./dist/*",
    "rebuild": "rm -r ./dist/* & webpack --config build/webpack.config.js"
  },
